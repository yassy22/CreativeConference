export async function getStaticPaths() {
  const groups = await fetchApi({
    endpoint: "groups",
    wrappedByKey: "data",
    query: {
        populate:[ "image","players"], },
  });

  const paths = groups.map((group, i) => ({
    params: { id: group.id.toString() },
    props: {
        group,
    },
  }));

  return paths;
}



