---
import kaisa from '../../assets/gif/kaisa_1.gif'
import jhin from '../../assets/gif/jhin_2.gif'

---



<div class="fold">
<div class="heading">
    <div class="discover">
        <h2>Discover</h2>
       <img class="kais_img" src={kaisa.src} alt="">
    </div>

    <div class="LeagueCon ">
        <h2>LeagueCon </h2>
       <img class="jhin_img" src={jhin.src} alt="">
    </div>
</div>



<style>
.heading{
    height: 100vh;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;

}
.discover{
  font-family: alphaLyrae;
  color: #D9D9D9;
  font-size: 200px;

  display: flex;
  gap: 50px;
}

.LeagueCon{
  font-family: MonumentExtended;
  color: #D9D9D9;
  font-size: 90px;

  display: flex;
  flex-direction: row-reverse;
  gap: 100px;

align-items: center;

}
</style>
